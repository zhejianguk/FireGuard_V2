{
    "name": "parsec",
    "base": "br-base.json",
    "overlay": "overlay",
    "host-init": "host-init.sh",
    "guest-init": "guest-init.sh",
    "outputs": [
        "/root"
    ],
    "rootfs-size": "500M",
    "jobs": [
        {
            "name": "all",
            "command": "cd /root/pkgs && ./run_parsec.sh -k sanitiser -b swaptions && poweroff -f"
        }
    ]
}